{
  "name": "@lumina-charts/solid",
  "version": "0.0.1",
  "description": "Solid.js wrapper for Lumina Charts",
  "author": "Lumina Charts",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "solid": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@lumina-charts/core": "workspace:*"
  },
  "devDependencies": {},
  "peerDependencies": {
    "solid-js": ">=1.0.0"
  },
  "keywords": [
    "charts",
    "solid",
    "solidjs",
    "lumina-charts",
    "webgl"
  ]
}
