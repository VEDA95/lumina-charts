{
  "name": "@lumina-charts/react",
  "version": "0.0.1",
  "description": "React wrapper for Lumina Charts with React 19 compiler support",
  "author": "Lumina Charts",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@lumina-charts/core": "workspace:*",
    "react-compiler-runtime": "19.1.0-rc.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "babel-plugin-react-compiler": "19.1.0-rc.3"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  },
  "keywords": [
    "charts",
    "react",
    "lumina-charts",
    "webgl",
    "react-19",
    "react-compiler"
  ]
}
